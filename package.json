{
  "name": "@topazjs/shared",
  "version": "0.0.1",
  "description": "topazjs - Commonly-used, shared, generic components",
  "main": "index.js",
  "private": false,
  "scripts": {
    "test": "jest -u",
    "reset": "rm -rf node_modules;yarn --force",
    "refresh": "npm run prep; npm run reset; npm run build-dist",
    "prep": "sh clean.sh dist",
    "copy-files": "cp -rf src/img src/webfonts dist/",
    "watch": "npm run prep && npm run copy-files; npm-run-all --parallel js-watch sass-watch css-watch",
    "build": "npm run prep && npm run copy-files; npm run js-build; npm run css-build",
    "build-dist": "npm run prep && npm run copy-files; npm-run-all --parallel js-build-dist css-build-dist; npm run js-min; npm run css-pure-dist",
    "js-build": "export NODE_ENV=development;browserify -s ${PWD##*/} -e src/js/App.js -v -d -t envify -t babelify | exorcist dist/js/app.js.map -u /${PWD##*/}/dist/js/app.js.map > dist/js/app.js",
    "js-build-dist": "export NODE_ENV=production;browserify -s ${PWD##*/} -e src/js/App.js -v -d -t envify -t babelify -t uglifyify -o /tmp/app-${PWD##*/}.js",
    "js-build-dist-with-maps": "export NODE_ENV=production;browserify -s ${PWD##*/} -e src/js/App.js -v -d -t envify -t babelify -t uglifyify | exorcist /tmp/app-${PWD##*/}.js.map > /tmp/app-${PWD##*/}.js",
    "js-min-old-with-maps": "uglifyjs /tmp/app-${PWD##*/}.js -m -c --in-source-map /tmp/app-${PWD##*/}.js.map --screw-ie8 --source-map dist/js/app.js.map --source-map-url /${PWD##*/}/dist/js/app.js.map > dist/js/app.js",
    "js-min": "uglifyjs /tmp/app-${PWD##*/}.js -m -c --screw-ie8 > dist/js/app.js",
    "js-watch": "export NODE_ENV=development;watchify -s ${PWD##*/} -e src/js/App.js -v -d -t envify -t babelify -o 'exorcist dist/js/app.js.map > dist/js/app.js'",
    "sass-build": "cd styles && node-sass --include-path ../node_modules sass/styles.scss css/styles.css",
    "sass-build-dist": "cd styles && node-sass --include-path ../node_modules --omit-source-map-url sass/styles.scss css/styles.css",
    "sass-watch": "npm run sass-build -- --watch",
    "css-watch": "node watch-css",
    "css-build": "npm run sass-build && node compile-css",
    "css-build-dist": "npm run sass-build && node compile-css dist",
    "css-pure": "purifycss dist/css/styles.css dist/js/app.js --out dist/css/styles.css",
    "css-pure-dist": "purifycss --min dist/css/styles.css dist/js/app.js --out dist/css/styles.css",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@babel/runtime-corejs2": "^7.8.3",
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@smooth-ui/core-sc": "^11.1.5",
    "classnames": "^2.2.6",
    "color": "^3.0.0",
    "date-fns": "^2.9.0",
    "dayjs": "^1.8.15",
    "fast-memoize": "^2.5.1",
    "fastdom": "^1.0.9",
    "flexboxgrid": "^6.3.1",
    "flow-bin": "^0.63.1",
    "immutable": "^4.0.0-rc.12",
    "mobx": "^5.15.3",
    "mobx-react": "^6.1.4",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "popper.js": "^1.14.3",
    "react": "^16.12.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-perf": "^15.4.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-beautiful-dnd": "^12.2.0",
    "react-contextmenu": "^2.9.2",
    "react-dom": "^16.12.0",
    "react-nice-dates": "^1.0.2",
    "react-popper": "^0.10.4",
    "react-redux": "^5.1.2",
    "react-transition-group": "^2.3.1",
    "react-virtualized": "^9.18.5",
    "react-windowed-list": "^1.5.7",
    "redux": "^3.7.2",
    "store": "^2.0.12",
    "styled-components": "^5.0.0",
    "sweetalert2": "^9.6.0",
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "autoprefixer": "^8.6.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-exponentiation-operator": "^6.13.0",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "babelify": "^10.0.0",
    "browserify": "^16.2.2",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.0",
    "envify": "^4.1.0",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "exorcist": "^1.0.1",
    "immutable-devtools": "^0.0.7",
    "jest": "^21.2.1",
    "mobx-react-devtools": "^4.2.15",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.23",
    "postcss-cli": "^5.0.1",
    "postcss-font-magician": "^2.2.1",
    "postcss-import": "^11.1.0",
    "postcss-js": "^2.0.0",
    "postcss-less": "^2.0.0",
    "postcss-less-engine": "^0.6.2",
    "postcss-url": "^7.3.2",
    "purify-css": "^1.2.5",
    "react-test-renderer": "^16.2.0",
    "uglify-js": "^3.4.1",
    "uglifyify": "^5.0.1",
    "watchify": "^3.11.0"
  }
}
