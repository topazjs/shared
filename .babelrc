{
    "presets": [
        "flow",
        ["es2015", {"loose": true}],
        "react"
    ],
    "plugins": [
        "transform-export-extensions",
        "transform-react-constant-elements",
        "transform-react-inline-elements",
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-decorators-legacy",
        "transform-node-env-inline"
    ]
}
